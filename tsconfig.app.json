{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts",
    "./node_modules/ngx-sweetalert2/*.ts",
    "./node_modules/ngx-sweetalert2/**/*.ts",
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "./node_modules/ngx-sweetalert2/**/*.spec.ts",
    "./node_modules/ngx-sweetalert2/**/*.e2e-spec.ts"
  ]
}
